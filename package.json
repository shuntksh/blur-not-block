{
  "name": "focusflow",
  "displayName": "Focusflow",
  "version": "0.0.6",
  "description": "Your digital ally for undistracted productivity, empowering you to surf smarter, work better, and reclaim your focus.",
  "author": "Shun Takahashi",
  "manifest": {
    "content_security_policy": {},
    "permissions": [
      "storage"
    ],
    "host_permissions": [
      "https://www.youtube.com/*",
      "https://www.tiktok.com/*",
      "https://www.reddit.com/*",
      "https://twitter.com/*",
      "https://www.amazon.com/*"
    ]
  },
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@tanstack/react-query": "4.29.15",
    "antd": "5.6.2",
    "daisyui": "3.1.5",
    "plasmo": "0.76.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@changesets/cli": "2.26.1",
    "@plasmohq/prettier-plugin-sort-imports": "3.6.4",
    "@tailwindcss/typography": "0.5.9",
    "@types/chrome": "0.0.237",
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "postcss": "^8.4.24",
    "postcss-import": "15.1.0",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "typescript": "5.1.3"
  },
  "renovate": {
    "extends": [
      "config:base"
    ]
  }
}
